{
  "name": "experiment-trpc-turbo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev:blog": "turbo run dev --scope blog",
    "dev:api": "turbo run dev --scope api"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.12",
    "@experiment/eslint-config-custom": "workspace:*",
    "postcss": "^8.4.16",
    "prettier": "latest",
    "tailwindcss": "^3.2.4",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.18.1"
}
